<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
    <servlet>
        <servlet-name>MyServlet</servlet-name>
        <servlet-class>com.example.ajender.locationtracker.MyServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Database</servlet-name>
        <servlet-class>com.example.ajender.locationtracker.Database</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MapRelated</servlet-name>
        <servlet-class>com.example.ajender.locationtracker.MapRelated</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MyServlet</servlet-name>
        <url-pattern>/myser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Database</servlet-name>
        <url-pattern>/db</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MapRelated</servlet-name>
        <url-pattern>/map</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>